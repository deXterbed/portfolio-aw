<div class="col-md-4">
  <div class="card" data-id="<%= portfolio_item.id  %>">
      <%= image_tag portfolio_img(portfolio_item.thumb_image, 'thumb'), width: '100%' %>
      <p class="card-text">
        <span><%= link_to portfolio_item.title, portfolio_show_path(portfolio_item.id) %></span>
        <span><%= portfolio_item.subtitle %></span>
        <%= link_to "show", portfolio_show_path(portfolio_item.id), class: "btn btn-primary" %>
      </p>
  </div>
</div>